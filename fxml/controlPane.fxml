<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.java.mp3player.controller.ControlPaneController">
    <children>
        <HBox alignment="CENTER_LEFT">
            <Button id="previous" fx:id="previousButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" HBox.hgrow="NEVER">
            <ToggleButton id="play" fx:id="playButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" HBox.hgrow="NEVER" />
            <Button id="next" fx:id="nextButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" HBox.hgrow="NEVER" />
            <Slider id="musci" fx:id="progressSlider" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="250.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </Slider>
            <Slider id="volume" fx:id="volumeSlider" maxHeight="35.0" maxWidth="150.0" prefHeight="35.0" prefWidth="64.0" HBox.hgrow="NEVER">
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </Slider>
        </HBox>
        <TextField fx:id="messageTextField" alignment="CENTER" text="Mp3Player v1.0" editable="false"/>
    </children>
</VBox>
